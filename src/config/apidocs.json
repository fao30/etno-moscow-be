{
	"openapi": "3.0.0",
	"info": {
		"title": "Etno-moscow",
		"description": "API for website etno",
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "http://localhost:3000",
			"description": "local 1"
		},
		{
			"url": "https://dev.etno-edu.com",
			"description": "development"
		}
	],
	"components": {
		"securitySchemes": {
			"bearer": {
				"type": "http",
				"scheme": "bearer"
			}
		}
	},
	"tags": [
		{
			"name": "Regions",
			"description": "All regions route"
		},
		{
			"name": "Questions",
			"description": "All questions route"
		},
		{
			"name": "Surveys",
			"description": "All surveys route"
		},
		{
			"name": "Register and Login",
			"description": "User register and login routes"
		}
	],
	"paths": {
		"/api/regions": {
			"get": {
				"tags": ["Regions"],
				"summary": "Get all Regions",
				"responses": {
					"200": {
						"description": "Get Regions success"
					},
					"400": {
						"description": "Bad request"
					}
				}
			}
		},
		"/api/regions/{id}": {
			"get": {
				"tags": ["Regions"],
				"summary": "Get Region by UUID",
				"security": [
					{
						"bearer": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Questions's UUID"
					}
				],
				"responses": {
					"200": {
						"description": "Get Region success"
					},
					"400": {
						"description": "Bad request"
					}
				}
			}
		},
		"/api/questions": {
			"get": {
				"tags": ["Questions"],
				"summary": "Get all Questions",
				"responses": {
					"200": {
						"description": "Get Questions success"
					},
					"400": {
						"description": "Bad request"
					}
				}
			}
		},
		"/api/questions/{id}": {
			"put": {
				"tags": ["Questions"],
				"summary": "Update question",
				"security": [
					{
						"bearer": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Questions's UUID"
					}
				],
				"requestBody": {
					"content": {
						"multipart/form-data": {
							"schema": {
								"type": "object",
								"properties": {
									"picture": {
										"type": "string",
										"format": "binary"
									},
									"name": {
										"type": "string"
									}
								}
							}
						},
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"questions": {
										"type": "string"
									},
									"questionType": {
										"type": "string"
									},
									"correctAnswer": {
										"type": "string"
									},
									"score": {
										"type": "integer"
									},
									"instituteId": {
										"type": "integer"
									},
									"answersArray": {
										"type": "string"
									},
									"surveyId": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "Success edit question"
					},
					"400": {
						"description": "Can't edit question"
					}
				}
			},
			"get": {
				"tags": ["Questions"],
				"summary": "Get Question by UUID",
				"security": [
					{
						"bearer": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Questions's UUID"
					}
				],
				"responses": {
					"200": {
						"description": "Get Questions success"
					},
					"400": {
						"description": "Bad request"
					}
				}
			},
			"delete": {
				"tags": ["Questions"],
				"summary": "Delete questions",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Questions's UUID"
					}
				],
				"responses": {
					"200": {
						"description": "Questions deleted"
					},
					"400": {
						"description": "Bad request"
					}
				}
			}
		}, 
		"/api/questions/{surveyId}": {
			"post": {
				"tags": ["Questions"],
				"summary": "Make new question based on survey Id",
				"parameters": [
					{
						"name": "surveyId",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Surveys's id"
					}
				],
				"requestBody": {
					"content": {
						"multipart/form-data": {
							"schema": {
								"type": "object",
								"properties": {
									"picture": {
										"type": "string",
										"format": "binary"
									},
									"name": {
										"type": "string"
									}
								}
							}
						},
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"questions": {
										"type": "string"
									},
									"questionType": {
										"type": "string"
									},
									"correctAnswer": {
										"type": "string"
									},
									"score": {
										"type": "integer"
									},
									"instituteId": {
										"type": "integer"
									},
									"answersArray": {
										"type": "string"
									},
									"surveyId": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "Success made question"
					},
					"400": {
						"description": "Can't question"
					}
				}
			}
		}, 
		"/api/register": {
			"post": {
				"tags": ["Register and Login"],
				"summary": " User register",
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"lastName": {
										"type": "string"
									},
									"firstName": {
										"type": "string"
									},
									"middleName": {
										"type": "string"
									},
									"dateOfBirth": {
										"type": "string"
									},
									"universityId": {
										"type": "integer"
									},
									"instituteId": {
										"type": "integer"
									},
									"majorId": {
										"type": "integer"
									},
									"specialtyId": {
										"type": "integer"
									},
									"educationId": {
										"type": "integer"
									},
									"departmentId": {
										"type": "integer"
									},
									"studyYear": {
										"type": "integer"
									},
									"position": {
										"type": "string"
									},
									"phone": {
										"type": "string"
									},
									"email": {
										"type": "string"
									},
									"password": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "Success registered"
					},
					"401": {
						"description": "Unauthorized"
					}
				}
			}
		},
		"/api/login": {
			"post": {
				"tags": ["Register and Login"],
				"summary": "User login using email and password",
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"email": {
										"type": "string"
									},
									"password": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "Login success"
					},
					"403": {
						"description": "Cannot login"
					},
					"404": {
						"description": "User not registered"
					}
				}
			}
		},
		"/api/surveys": {
			"get": {
				"tags": ["Surveys"],
				"summary": "Get all Surveys",
				"responses": {
					"200": {
						"description": "Get Surveys success"
					},
					"400": {
						"description": "Bad request"
					}
				}
			},
			"post": {
				"tags": ["Surveys"],
				"summary": "Make new Survey",
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"title": {
										"type": "string"
									},
									"descriptions": {
										"type": "string"
									},
									"correctAnswer": {
										"type": "string"
									},
									"maxScore": {
										"type": "integer"
									},
									"isPrivate": {
										"type": "boolean"
									},
									"isOpen": {
										"type": "boolean"
									},
									"regionId": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "Success made question"
					},
					"400": {
						"description": "Can't question"
					}
				}
			}
		},
		"/api/surveys/{id}": {
			"put": {
				"tags": ["Surveys"],
				"summary": "Update survey",
				"security": [
					{
						"bearer": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Surveys's UUID"
					}
				],
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"title": {
										"type": "string"
									},
									"descriptions": {
										"type": "string"
									},
									"correctAnswer": {
										"type": "string"
									},
									"maxScore": {
										"type": "integer"
									},
									"isPrivate": {
										"type": "boolean"
									},
									"isOpen": {
										"type": "boolean"
									},
									"regionId": {
										"type": "string"
									}
								}
							}
						}
					}
				},
				"responses": {
					"201": {
						"description": "Success edit survey"
					},
					"400": {
						"description": "Can't edit survey"
					}
				}
			},
			"get": {
				"tags": ["Surveys"],
				"summary": "Get survey by UUID",
				"security": [
					{
						"bearer": []
					}
				],
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Survey's UUID"
					}
				],
				"responses": {
					"200": {
						"description": "Get Survey success"
					},
					"400": {
						"description": "Bad request"
					}
				}
			},
			"delete": {
				"tags": ["Surveys"],
				"summary": "Delete survey",
				"parameters": [
					{
						"name": "id",
						"in": "path",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "Surveys's UUID"
					}
				],
				"responses": {
					"200": {
						"description": "Survey deleted"
					},
					"400": {
						"description": "Bad request"
					}
				}
			}
		}
	}
}
